{
  "name": "p5.js Inspector",
  "manifest_version": 2,
  "description": "Let people manipulate shapes on the canvas in real time using their p5 sketches",
  "version": "0.4",
  "background":{
    "scripts": [
        "background.js"
      ] 
    },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content_script.js"],
    "run_at": "document_end"
    // "css": ["style.css"]
  }],
  "web_accessible_resources": ["ace.js", "mode-javascript.js", "worker-javascript.js", "p5.inspector.js"],
  "browser_action":{
    "default_icon": "icon.png",
    "default_titile": "p5.js Inspector"
  }
}
